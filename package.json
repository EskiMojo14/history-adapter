{
  "name": "history-adapter",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./creator": {
      "import": "./dist/creator.js",
      "require": "./dist/creator.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "npm run type",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint",
    "pre-commit": "lint-staged",
    "attw": "attw",
    "publint": "publint",
    "type": "tsc",
    "prepack": "npm run publint"
  },
  "repository": "https://github.com/EskiMojo14/history-adapter.git",
  "author": "EskiMojo14 <ben.j.durrant@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.5",
    "@reduxjs/toolkit": "https://pkg.csb.dev/reduxjs/redux-toolkit/commit/a2660af0/@reduxjs/toolkit/_pkg.tgz",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "publint": "^0.2.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  },
  "prettier": {},
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/creator.ts"
    ],
    "sourcemap": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "minify": true
  },
  "dependencies": {
    "immer": "^10.0.3"
  }
}
